{
  "name": "CSRT-Frontend",
  "version": "1.0.0",
  "description": "CSRT Frontend",
  "main": "app.js",
  "author": "Kevin Old <kevinold@gmail.com>",
  "license": "MIT",
  "config": {},
  "scripts": {
    "pack:full": "rimraf node_modules node_cache node_shrinkwrap npm-shrinkwrap.json && npm install && npm shrinkwrap && shrinkpack .",
    "pack": "npm shrinkwrap && shrinkpack .",
    "eslint:all": "eslint src/client/js || true",
    "stylelint": "stylelint \"src/client/css/**/*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "validate": "npm run lint",
    "test": "NODE_ENV=test karma start --singleRun true",
    "test:sauce": "BUILD_NUMBER=1 NODE_ENV=test karma start --singleRun true",
    "test:firefox": "NODE_ENV=test karma start --singleRun true --browsers Firefox",
    "test:safari": "NODE_ENV=test karma start --singleRun true --browsers Safari",
    "test:watch": "NODE_ENV=test karma start",
    "test:watch:firefox": "NODE_ENV=test karma start --browsers Firefox",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack -p --release",
    "webpack:validate": "webpack-validator webpack.config.js",
    "check": "npm-check --skip-unused",
    "nsp": "nsp check",
    "mkprepush": "echo '#!/bin/sh\nnpm test\nexit 0' > .git/hooks/pre-push; chmod 755 .git/hooks/pre-push",
    "prestart": "npm run clean",
    "start": "browser-sync start --config 'bs-config.js'"
  },
  "engines": {
    "node": ">=6",
    "npm": "<3.8.8 >=3.10.4"
  },
