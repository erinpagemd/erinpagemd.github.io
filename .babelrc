{
  "presets": ["react", "es2015", "es2016", "es2017"],
  "plugins": [
    "syntax-async-functions",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "presets": ["airbnb"],
      "plugins": [["__coverage__", { "only", "src/" }]]
    }
  }
}
